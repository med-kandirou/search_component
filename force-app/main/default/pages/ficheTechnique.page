<apex:page renderAs="PDF" controller="FicheTechniqueController">
    
    <apex:form >
        <!-- Display the data -->
        <apex:dataTable value="{!data}" var="prod">
            <apex:column value="{!prod.Name}" />
            <apex:column value="{!prod.Marque__c}" />
            <apex:column value="{!prod.modele__c}" />
            <apex:column value="{!prod.Serie__c}" />
        </apex:dataTable>
    </apex:form>
</apex:page>